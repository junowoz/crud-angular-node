//ARQUIVO PRISMA - ORM

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//Aqui Ã© onde criamos as tabelas do banco de dados
model Colaborador {
  id    Int    @id @default(autoincrement())
  cpf   String @unique
  nome  String
  cargo String
  setor String
}
